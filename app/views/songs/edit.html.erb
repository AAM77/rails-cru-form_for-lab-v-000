<h1>Edit Song</h1>

<h3><%= @song.name %></h3>

<%= form_for(@song) do |f| %>
  <label>Song title:</label><br>
  <%= f.text_field :name %><br>
  
  <label>Artist name:</label><br>
  <%= f.number_field :artist_name %><br>
  
  <label>Genre name:</label><br>
  <%= f.number_field :genre_name %><br>
  
  <%= f.submit %>
<% end %>