<h1>Create New Song</h1>

<%= form_for(@song) do |f| %>
  <label>Song title:</label><br>
  <%= f.text_field :name %><br>
  
  <label>Artist ID:</label><br>
  <%= f.number_field :artist_id %><br>
  
  <label>Genre ID:</label><br>
  <%= f.number_field :genre_id %><br>
  
  <label>Artist name:</label><br>
  <%= f.text_field :artist %><br>
  
  <label>Genre name:</label><br>
  <%= f.text_field :genre %><br>
  
  <%= f.submit %>
<% end %>
